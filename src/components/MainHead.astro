---
import "@/styles/global.css";

interface Props {
    title?: string,
    description?: string;
}

const {
    title = "Estadísticas - Hera Network",
    description = "Estas son las estadísticas"
} = Astro.props
---
<meta charset="UTF-8" />
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<link rel="icon" type="image/svg+xml" href="/assets/Thophy.svg" />
<meta name="generator" content={Astro.generator} />
<meta name="description" content={description}>
<meta name="author" content="stephanofer"/>
<meta name="robots" content="index, follow" />
<meta
  name="keywords"
  content="heranetwork"
/>

<link rel="preload" href="/fonts/Inter.woff2" as="font" type="font/woff2" crossorigin="anonymous">
<link rel="preload" href="/fonts/Clash.woff2" as="font" type="font/woff2" crossorigin="anonymous" >
<link rel="preload" href="/fonts/Seven.woff2" as="font" type="font/woff2" crossorigin="anonymous">


<title>{title}</title>